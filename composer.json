{
  "name": "hunternnm/laravel-roadrunner",
  "description": "Bridge for high-performance PHP application server - RoadRunner",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Porozhnyy",
      "email": "hunter653@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "illuminate/console": "^5.5",
    "illuminate/contracts": "^5.5",
    "illuminate/http": "^5.5",
    "illuminate/routing": "^5.5",
    "illuminate/support": "^5.5",
    "spiral/roadrunner": "^2.0",
    "nyholm/psr7": "^1.4",
    "symfony/psr-http-message-bridge": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hunternnm\\LaravelRoadrunner\\RoadrunnerServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Hunternnm\\LaravelRoadrunner\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "bin": [
    "bin/roadrunner-worker"
  ]
}
